usethis::use_r("linReg")
data <- data.frame(y = c(1:100), x = c(1:100))
plot(data$y~data$x)
rnorm(100)
data <- data.frame(y = c(1:100) + rnorm(100), x = c(1:100) + rnorm(100))
plot(data$y~data$x)
mod <- lm(data$x ~ data$y)
print(summary(mod))
plot(data$y~data$x)
abline(mod)
linReg <- function(data, save.result = NULL){
mod <- lm(data$x ~ data$y)
print(summary(mod))
plot(data$y~data$x)
abline(mod)
# plotly::plot_ly(data, x = ~x, y = ~y, type = "scatter")
if (!is.null(save.result)){
out <- data.frame(beta_0 = coef(mod)[1],
beta_1 = coef(mod)[2],
R2 = summary(mod)$adj.r.squared)
write.csv(out, "out.csv", row.names = FALSE)
}
# res(mod)
}
linReg(data)
linReg(data)
